<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:t='urn:import:com.instajet.gwt.client.ui'
	xmlns:m='urn:import:gwt.material.design.client.ui'>




	<g:HTMLPanel>

		<m:MaterialContainer>
		</m:MaterialContainer>
		<m:MaterialRow>
		<m:MaterialColumn grid="s6" offset="s3">
		<m:MaterialCard backgroundColor="BLUE_GREY_DARKEN_1">
            <m:MaterialCardContent textColor="WHITE">
              <m:MaterialCardTitle text="RPC Connection"/>
              <m:MaterialTextBox ui:field="username" label="Username" />
              <m:MaterialTextBox ui:field="password" label="Password" type="PASSWORD"/>
            </m:MaterialCardContent>
            <m:MaterialCardAction>
              <m:MaterialLink ui:field="btn" text="Send" iconType="SEND" iconPosition="LEFT"/>
            </m:MaterialCardAction>
          </m:MaterialCard>
          </m:MaterialColumn>
          </m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder>